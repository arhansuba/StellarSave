# .env.local
# StellarSave Environment Variables
# Copy this file and update with your deployed contract addresses

# ===== CONTRACT ADDRESSES =====
# These will be set automatically by the deployment script
# Or update manually after deploying contracts

# Main StellarSave contracts (deployed via deploy-stellarsave.sh)
VITE_SAVINGS_CONTRACT_ID=
VITE_REWARD_TOKEN_CONTRACT_ID=

# Existing SEP wallet contracts (if using with SEP wallet)
VITE_WEBAUTH_VERIFY_CONTRACT_ID=
VITE_WEBAUTHN_FACTORY_CONTRACT_ID=
VITE_WEBAUTHN_WALLET_CONTRACT_ID=

# ===== STELLAR NETWORK CONFIGURATION =====

# Network selection: "testnet" or "mainnet"
VITE_STELLAR_NETWORK=testnet

# Network passphrases
VITE_STELLAR_NETWORK_PASSPHRASE=Test SDF Network ; September 2015
# For mainnet use: Public Global Stellar Network ; September 2015

# RPC URLs
VITE_STELLAR_RPC_URL=https://soroban-testnet.stellar.org:443
# For mainnet use: https://soroban-rpc.mainnet.stellar.org:443

# Horizon URLs (for account info)
VITE_STELLAR_HORIZON_URL=https://horizon-testnet.stellar.org
# For mainnet use: https://horizon.stellar.org

# ===== STELLARSAVE CONFIGURATION =====

# Feature flags
VITE_STELLARSAVE_ENABLED=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=false

# Challenge limits
VITE_MIN_CHALLENGE_AMOUNT=10
VITE_MAX_CHALLENGE_AMOUNT=100000
VITE_DEFAULT_CHALLENGE_DURATION=12
VITE_MAX_PARTICIPANTS=20
VITE_MAX_CHALLENGE_DURATION=104

# Reward configuration
VITE_BASE_WEEKLY_REWARD=10
VITE_MILESTONE_MULTIPLIER=1.5
VITE_COMPLETION_MULTIPLIER=2.0
VITE_MAX_STREAK_BONUS=50

# ===== UI CONFIGURATION =====

# Theme and styling
VITE_THEME=light
VITE_PRIMARY_COLOR=blue
VITE_ENABLE_DARK_MODE=true

# Localization
VITE_DEFAULT_LOCALE=en-US
VITE_DEFAULT_CURRENCY=XLM

# Data refresh intervals (in milliseconds)
VITE_REFRESH_INTERVAL_CHALLENGES=30000
VITE_REFRESH_INTERVAL_STATS=45000
VITE_REFRESH_INTERVAL_BALANCE=30000

# ===== DEVELOPMENT SETTINGS =====

# Mock data (for development without deployed contracts)
VITE_USE_MOCK_DATA=false
VITE_MOCK_USER_ADDRESS=GXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Performance monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_LOG_LEVEL=info

# Error tracking
VITE_ENABLE_ERROR_TRACKING=false
VITE_SENTRY_DSN=

# ===== EXTERNAL INTEGRATIONS =====

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Support and help
VITE_SUPPORT_EMAIL=support@stellarsave.com
VITE_DOCS_URL=https://docs.stellarsave.com
VITE_DISCORD_URL=https://discord.gg/stellarsave

# ===== SECURITY SETTINGS =====

# CORS settings (for production)
VITE_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate limiting
VITE_MAX_REQUESTS_PER_MINUTE=60
VITE_MAX_CONCURRENT_REQUESTS=10

# Transaction limits
VITE_MAX_TRANSACTION_AMOUNT=1000000
VITE_MIN_TRANSACTION_AMOUNT=0.0000001

# ===== NOTIFICATIONS =====

# Browser notifications
VITE_ENABLE_BROWSER_NOTIFICATIONS=true
VITE_NOTIFICATION_PERMISSION_PROMPT=true

# Push notifications (future feature)
VITE_ENABLE_PUSH_NOTIFICATIONS=false
VITE_VAPID_PUBLIC_KEY=

# ===== ADVANCED CONFIGURATION =====

# Smart contract interaction
VITE_DEFAULT_GAS_LIMIT=1000000
VITE_GAS_PRICE_MULTIPLIER=1.1
VITE_TRANSACTION_TIMEOUT=30000

# Retry logic
VITE_MAX_RETRY_ATTEMPTS=3
VITE_RETRY_DELAY=1000
VITE_EXPONENTIAL_BACKOFF=true

# Caching
VITE_CACHE_TTL=300000
VITE_ENABLE_QUERY_CACHE=true
VITE_CACHE_SIZE_LIMIT=50

# ===== FEATURE FLAGS =====

# Experimental features
VITE_ENABLE_CHALLENGE_TEMPLATES=false
VITE_ENABLE_SOCIAL_SHARING=false
VITE_ENABLE_CHALLENGE_COMMENTS=false
VITE_ENABLE_LEADERBOARDS=false

# Beta features
VITE_ENABLE_MULTI_CURRENCY=false
VITE_ENABLE_RECURRING_CHALLENGES=false
VITE_ENABLE_CHALLENGE_MARKETPLACE=false

# ===== DEPLOYMENT INFO =====

# Build information (set automatically)
VITE_BUILD_VERSION=1.0.0
VITE_BUILD_TIMESTAMP=
VITE_GIT_COMMIT=
VITE_DEPLOYMENT_ENV=development

